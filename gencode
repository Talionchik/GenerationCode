# Небольшая программа для создания пароля

import random
import time

#Ниже переменные с набором символов
a = "abcdefghijklmnopqrstuvwxyz" 
b = "ABCDEFGHIJKLMNOPRQRSTUVWXYZ"
c = "0123456789"
d = "[]{}()*?,.<>/|!"

all = a + b + c + d
length = 12
password = "".join(random.sample(all, length))
print("Создается секретный пароль...")
time.sleep(5)

print(password)
input()
